{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest"
    },

    "rules": {
      "no-underscore-dangle": [
        "off",
        {
          "allow": [
            "id"
          ]
        }
      ]
    }
}

